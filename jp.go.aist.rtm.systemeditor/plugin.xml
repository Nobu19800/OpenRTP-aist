	<extension
		point="org.eclipse.core.expressions.definitions">
		<definition
			id="rtse.definition.activeSystemDiagramEditor">
			<with variable="activeEditorId">
				<equals value="jp.go.aist.rtm.systemeditor.ui.editor.SystemDiagramEditor"/>
			</with>
		</definition>
		<definition
			id="rtse.definition.activeOfflineSystemDiagramEditor">
			<with variable="activeEditorId">
				<equals value="jp.go.aist.rtm.systemeditor.ui.editor.OfflineSystemDiagramEditor"/>
			</with>
		</definition>
		<definition
			id="rtse.definition.selectComponent">
			<with variable="selection">
				<iterate ifEmpty="false" operator="or">
					<instanceof value="jp.go.aist.rtm.systemeditor.ui.editor.editpart.ComponentEditPart"/>
				</iterate>
			</with>
		</definition>
		<definition
			id="rtse.definition.selectEC">
			<with variable="selection">
				<iterate ifEmpty="false" operator="or">
					<instanceof value="jp.go.aist.rtm.systemeditor.ui.editor.editpart.ECEditPart"/>
				</iterate>
			</with>
		</definition>
		<definition
			id="rtse.definition.selectOwnEC">
			<with variable="selection">
				<iterate ifEmpty="false" operator="or">
					<instanceof value="jp.go.aist.rtm.systemeditor.ui.editor.editpart.ECEditPart$OwnECEditPart"/>
				</iterate>
			</with>
		</definition>
		<definition
			id="rtse.definition.selectPartEC">
			<with variable="selection">
